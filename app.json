{
  "expo": {
    "name": "medicAction",
    "slug": "medicAction",
    "version": "1.0.0",
    "main": "App.js",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medicaction",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourdomain.medicaction"
    },
    "android": {
      "package": "com.yourdomain.medicaction",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "pwa": {
        "name": "medicAction",
        "shortName": "medicAction",
        "startUrl": ".",
        "backgroundColor": "#ffffff",
        "themeColor": "#000000",
        "display": "standalone",
        "scope": "/",
        "icons": [
          {
            "src": "./assets/images/pwa-icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/images/pwa-icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ],
        "serviceWorker": {
          "src": "/service-worker.js",
          "scope": "/"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
